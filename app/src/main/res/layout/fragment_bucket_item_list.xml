    android:layout_height="match_parent"
    android:layout_marginLeft="16dp"
    android:layout_marginRight="16dp"
    tools:context=".fragment.BucketListItemFragment"
    tools:listitem="@layout/layout_bucket_item" />

</LinearLayout>